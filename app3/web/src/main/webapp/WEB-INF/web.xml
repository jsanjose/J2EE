<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <display-name>Archetype Created J2EE Application</display-name>

    <!-- HolaServlet -->
    <servlet>
        <servlet-name>HolaServlet</servlet-name>
        <servlet-class>hola.HolaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HolaServlet</servlet-name>
        <url-pattern>/hola</url-pattern>
    </servlet-mapping>

    <!-- SQLServlet -->
    <servlet>
        <servlet-name>SQLServlet</servlet-name>
        <servlet-class>hola.SQLServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SQLServlet</servlet-name>
        <url-pattern>/sql</url-pattern>
    </servlet-mapping>

</web-app>