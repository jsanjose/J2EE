<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <display-name>J2EE Application</display-name>

    <servlet>
        <servlet-name>HolaServlet</servlet-name>
        <servlet-class>hola.HolaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HolaServlet</servlet-name>
        <url-pattern>/hola</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NominaServlet</servlet-name>
        <servlet-class>fechas.NominaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NominaServlet</servlet-name>
        <url-pattern>/nomina</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FormularioServlet</servlet-name>
        <servlet-class>datos.FormularioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormularioServlet</servlet-name>
        <url-pattern>/formulario</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>DatosServlet</servlet-name>
        <servlet-class>datos.DatosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DatosServlet</servlet-name>
        <url-pattern>/datos</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FechasServlet</servlet-name>
        <servlet-class>fechas.FechasServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FechasServlet</servlet-name>
        <url-pattern>/fechas</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>sesion.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

</web-app>